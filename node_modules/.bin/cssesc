#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/anar/Music/anar/gh/davlux-test/themes/Davlux/node_modules/cssesc/bin/node_modules:/home/anar/Music/anar/gh/davlux-test/themes/Davlux/node_modules/cssesc/node_modules:/home/anar/Music/anar/gh/davlux-test/themes/Davlux/node_modules:/home/anar/Music/anar/gh/davlux-test/themes/node_modules:/home/anar/Music/anar/gh/davlux-test/node_modules:/home/anar/Music/anar/gh/node_modules:/home/anar/Music/anar/node_modules:/home/anar/Music/node_modules:/home/anar/node_modules:/home/node_modules:/node_modules:/home/anar/Music/anar/gh/davlux-test/themes/Davlux/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/anar/Music/anar/gh/davlux-test/themes/Davlux/node_modules/cssesc/bin/node_modules:/home/anar/Music/anar/gh/davlux-test/themes/Davlux/node_modules/cssesc/node_modules:/home/anar/Music/anar/gh/davlux-test/themes/Davlux/node_modules:/home/anar/Music/anar/gh/davlux-test/themes/node_modules:/home/anar/Music/anar/gh/davlux-test/node_modules:/home/anar/Music/anar/gh/node_modules:/home/anar/Music/anar/node_modules:/home/anar/Music/node_modules:/home/anar/node_modules:/home/node_modules:/node_modules:/home/anar/Music/anar/gh/davlux-test/themes/Davlux/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../cssesc/bin/cssesc" "$@"
else
  exec node  "$basedir/../cssesc/bin/cssesc" "$@"
fi
